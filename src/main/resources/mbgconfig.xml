<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- mvn mybatis-generator:generate -->
    <context id="Mysql" targetRuntime="MyBatis3" defaultModelType="flat">
        <!-- 生成的文件编码 -->
        <property name="javaFileEncoding" value="utf-8"/>

        <!-- 注解 type="com.closet.ios.backend.MyCommentGenerator" -->
        <commentGenerator>
            <!-- <property name="suppressDate" value="true" /> -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- JDBC 连接信息 -->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/liangying?serverTimezone=UTC&amp;tinyInt1isBit=false"
                        userId="root" password="970512">
            <!-- 针对mysql数据库 -->
            <property name="useInformationSchema" value="true"></property>
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.springboot.framework.dao.entity"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>


        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.springboot.framework.dao.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>


        <!-- 数据表 -->
